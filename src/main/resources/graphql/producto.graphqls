type Producto {
    id: ID
    descripcion: String
    precioCompra: Float
    precioVenta: Float
    stock: Int
    activo: Boolean
    categoria: Categoria
}
type PaginadorProducto {
    items: [Producto!]!
    totalItems: Int!
    totalPages: Int!
    currentPage: Int!
    itemsCount: Int!
}
input InputProducto {
    descripcion: String!
    precioCompra: Float
    precioVenta: Float
    stock: Int
    activo: Boolean
    categoriaId: Int!
}