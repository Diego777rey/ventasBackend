type Usuario {
    id: ID!
    nombre: String!
    contrasenha: String!
    email: String!
    rol: String!
}
input InputUsuario {
    nombre: String!
    contrasenha: String!
    email: String!
    rol: String!
}
type PaginadorUsuario {
    items: [Usuario!]!
    totalItems: Int!
    totalPages: Int!
    currentPage: Int!
    itemsCount: Int!
}